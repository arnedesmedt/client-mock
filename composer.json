{
    "name": "arnedesmedt/client-mock",
    "type": "library",
    "description": "Mock REST and SOAP clients generated by arnedesmedt/openapi-codegen and arnedesmedt/soap-client",
    "homepage": "https://github.com/arnedesmedt/client-mock",
    "license": "MIT",
    "authors": [
        {
            "name": "Arne De Smedt",
            "email": "arnedesmdt@gmail.com",
            "homepage": "https://github.com/arnedesmedt"
        }
    ],
    "require": {
        "php": "^8",
        "arnedesmedt/php-util": "~0.1",
        "mockery/mockery": "~1.0",
        "arnedesmedt/value-objects": "~0.1"
    },
    "require-dev": {
        "phpro/grumphp": "~1.0",
        "phpstan/phpstan": "~1.0",
        "doctrine/coding-standard": "~9.0",
        "rector/rector": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "ADS\\ClientMock\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    }
}
